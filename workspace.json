      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/pusher/socketio/client/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/pusher/socketio/client"
          ],
          "options": {
            "jestConfig": "libs/pusher/socketio/client/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
