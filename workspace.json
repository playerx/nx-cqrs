{
  "version": 2,
  "projects": {
    "cqrs-rpc": {
      "root": "libs/cqrs/rpc",
      "sourceRoot": "libs/cqrs/rpc/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/cqrs/rpc/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/cqrs/rpc"
          ],
          "options": {
            "jestConfig": "libs/cqrs/rpc/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/cqrs/rpc",
            "tsConfig": "libs/cqrs/rpc/tsconfig.lib.json",
            "packageJson": "libs/cqrs/rpc/package.json",
            "main": "libs/cqrs/rpc/src/index.ts",
            "assets": [
              "libs/cqrs/rpc/*.md"
            ]
          }
        }
      }
    },
