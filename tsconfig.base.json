{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nx-cqrs/example/pusher/api": [
        "libs/example/pusher/api/src/index.ts"
      ],
      "@nx-cqrs/example/user/api": [
        "libs/example/user/api/src/index.ts"
      ],
      "@nx-cqrs/example/user/core": [
        "libs/example/user/core/src/index.ts"
      ],
      "@cheep/microservice": [
        "libs/microservice/src/index.ts"
      ],
      "@cheep/transport": [
        "libs/transport/core/src/index.ts"
      ],
      "@cheep/transport-rabbitmq": [
        "libs/transport/rabbitmq/src/index.ts"
      ],
      "@cheep/transport-aws": [
        "libs/transport/aws/src/index.ts"
      ],
      "@cheep/transport/shared": [
        "libs/transport/shared/src/index.ts"
      ],
      "@cheep/microservices": [
        "libs/microservices/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
